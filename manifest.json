{
    "manifest_version": 2,
    "name": "Game of Ping Pong",
    "version": "1.0",
    "description": "Play Ping Pong",
    "icons": {
        "128": "img/pingpong128.png",
        "48": "img/pingpong48.png",
        "16": "img/pingpong16.png"
    },
    "browser_action": {
        "default_icon": "img/pingpong16.png",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}